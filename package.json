{
  "name": "fly-helper",
  "version": "1.0.1",
  "description": "工具库",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.js",
  "files": [
    "lib",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "sideEffects": "false",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonwong/fly-helper.git"
  },
  "author": "Simon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonwong/fly-helper/issues"
  },
  "homepage": "https://github.com/simonwong/fly-helper#readme",
  "scripts": {
    "build": "gulp build",
    "api": "api-extractor run",
    "test": "jest --coverage --verbose -u",
    "lint": "eslint --ext .js,.ts ./src",
    "lint:fix": "eslint --fix --ext .js,.ts ./src",
    "changelog": "gulp changelog",
    "version": "yarn changelog && git add CHANGELOG.md",
    "release": "yarn test && yarn build && np --no-cleanup --yolo --any-branch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.7",
    "@microsoft/api-extractor": "^7.7.0",
    "@types/fs-extra": "^9.0.1",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.0.31",
    "eslint": "^7.6.0",
    "eslint-plugin-jest": "^23.1.1",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "np": "^6.3.2",
    "rollup": "^2.23.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & jest -u"
    }
  },
  "lint-staged": {
    "*.{.ts,.js}": [
      "eslint --ext .js,.ts ./src"
    ]
  }
}
