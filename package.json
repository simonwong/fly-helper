{
  "name": "fly-helper",
  "version": "1.1.0",
  "description": "工具库",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "sideEffects": "false",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonwong/fly-helper.git"
  },
  "author": "Simon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonwong/fly-helper/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/simonwong/fly-helper#readme",
  "scripts": {
    "build": "gulp build",
    "api": "api-extractor run",
    "test": "jest --coverage --verbose -u",
    "lint": "eslint --ext .js,.ts --format=pretty ./src",
    "lint:fix": "eslint --fix --ext .js,.ts --format=pretty ./src",
    "changelog": "gulp changelog",
    "version": "yarn changelog && git add CHANGELOG.md",
    "release": "yarn test && yarn build && np --no-cleanup --yolo --any-branch",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@microsoft/api-extractor": "^7.18.17",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@yueqing/lint": "^1.4.0",
    "chalk": "^4.1.2",
    "conventional-changelog": "^3.1.24",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.1.2",
    "np": "^7.5.0",
    "rollup": "^2.59.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.0"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix --format=pretty"
    ]
  }
}
