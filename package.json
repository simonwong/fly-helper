{
  "name": "fly-helper",
  "version": "1.0.1",
  "description": "工具库",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.js",
  "files": [
    "lib",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "sideEffects": "false",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simonwong/fly-helper.git"
  },
  "author": "Simon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simonwong/fly-helper/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/simonwong/fly-helper#readme",
  "scripts": {
    "build": "gulp build",
    "api": "api-extractor run",
    "test": "jest --coverage --verbose -u",
    "lint": "eslint --ext .js,.ts --format=pretty ./src",
    "lint:fix": "eslint --fix --ext .js,.ts --format=pretty ./src",
    "changelog": "gulp changelog",
    "version": "yarn changelog && git add CHANGELOG.md",
    "release": "yarn test && yarn build && np --no-cleanup --yolo --any-branch"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@microsoft/api-extractor": "^7.9.22",
    "@types/fs-extra": "^9.0.1",
    "@types/gulp": "^4.0.6",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@yueqing/lint": "^1.1.1",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.1.0",
    "fs-extra": "^9.0.1",
    "gulp": "^4.0.2",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "np": "^6.5.0",
    "rollup": "^2.28.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & jest -u"
    }
  },
  "lint-staged": {
    "*.{.ts,.js}": [
      "eslint --fix --format=pretty"
    ]
  }
}
